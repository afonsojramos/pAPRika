{
	"name": "diogocampos-msc",
	"displayName": "diogocampos-msc",
	"description": "Diogo Campos MSc Thesis",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"capabilities": {
		"codeActionProvider": "true"
	},
	"activationEvents": [
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.runCodeAction",
				"title": "Suggest Changes"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"mocha": "^6.0.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"ts-emitter": "^0.4.0"
	}
}
